name: Loan Prediction CI

on:
  push:
    branches: [ "main" ]

jobs:
  build:
    runs-on: ubuntu-latest

    steps:
      - name: Checkout repository
        uses: actions/checkout@v4

      - name: Verify project files
        run: |
          echo "Listing files"
          ls

      - name: CI completed
        run: echo "Build Successful"
